4.
n = int(input('n: '))
names = []
for i in range(n):
    name = input('Nhap ten: ')
    for a in name:
        if a[0] and a[-1] == 'T' and 'i':
            names.append(name)
print(names)
print()
5.
n = int(input())
a = 0
tong = 0
while a != n:
    name = input('Nhap ten: ')
    tong = tong + len(name)
    a = a + 1
print('Tong so luong chu cai: ', tong)
if tong > 100:
    print('True (vi lon hon 100)')
else:
    print('False (vi khong lon hon 100)')
6.
n = int(input('n: '))
numbers = []
for i in range(n):
    number = int(input('Nhap so: '))
    numbers.append(number)
for number in numbers:
    if number > 10 and number % 2 == 0:
        print(number)
