n = int(input('n: '))
names = []
for i in range(n):
    name = input('Nhap ten: ')
    names.append(name)
for name in names:
    if len(name) > 5:
        print(name)
    else:
        pass
2.
ds1 = int(input('danh sach 1: '))
letters1 = []
for i in range(ds1):
    letter1 = input('Nhap chu cai: ')
    letters1.append(letter1)
ds2 = int(input('danh sach 2: '))
letters2 = []
for a in range(ds2):
    letter2 = input('Nhap chu cai: ')
    letters2.append(letter2)
for letter1 in letters1:
    for letter2 in letters2:
        if letter2 == letter1:
            print('Chu cai trung: ', letter2)
        else:
            pass
