for i in names:
    if len(i) >= 4:
        print(i)
5.
from turtle import*
import turtle
t = Turtle()
t.speed(0)
a = int(input("Choose how many squares you want to draw : "))
for i in range(a):
    t.fd(20)
    t.lt(90)
    t.fd(20)
    t.lt(90)
    t.fd(20)
    t.lt(90)
    t.fd(20)
    t.lt(90)
    t.fd(20)
    t.rt(90)
    t.penup()
    t.fd(20)
    t.lt(90)
    t.back(20)
    t.rt(90)
    t.fd(20)
    t.lt(90)
    t.pendown()

done()
6.
from turtle import*
import turtle
t = Turtle()
t.speed(100)
for a in range(10):
    for i in range(10):
        t.speed(0)
        t.rt(300)
        t.fd(50)
        t.rt(120)

        t.fd(50)
        t.rt(120)
        t.rt(300)
        t.fd(50)
        t.rt(120)

        t.fd(50)
        t.rt(120)

    t.rt(45)
    t.penup()
    t.fd(50)
    t.pendown()
done()


7.
from turtle import*
import turtle
t = Turtle()
t.speed(0)
for a in range(1,100):
    for i in range(1 ,6):
        t.rt(90)
        t.fd(20)
    t.penup()
    t.rt(130)
    t.fd(200)
    t.pendown()

done()






9.
from turtle import*
import turtle
t = Turtle()
t.speed(0)
for a in range(0 , 300 , 20):
    t.circle(a)
    t.penup()
    t.goto(x=0 , y = -a)
    t.pendown()
done()
